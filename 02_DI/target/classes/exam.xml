<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="exam" class="com.khie.di03.Exam">
		<!-- Exam클래스에 setter가 존재하기 때문에 property도 가능 -->
		<!-- <property name=""></property> -->
		
		<!-- 인자 생성자를 이영하여 주입하는 방법  -->
		<constructor-arg value="안녕하세요! 스프링에 오신걸 환영합니다."/>
		<!-- 생성자의 인자가 일반 리터럴값은 상관없지만 객체일 경우에는 bean에서 객체를 정의해 준 뒤 ref태그를 통해 정의해 준다. -->
	</bean>

</beans>
